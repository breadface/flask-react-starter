    "test": "jest",
    "test-cov": "jest --coverage"
  },
  "author": {
    "name": "Omonori  Jesmine",
    "email": "damilolao328@gmail.com",
    "url": "https://github.com/breadface"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-test-renderer": "^15.4.2",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "jest": "^18.1.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/frontend/src/app/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/frontend/src/app/__mocks__/styleMock.js"
    }
  }
