  "description": "A starter kit for a react project served by flask",
  "engines": {
    "node": ">= 7.2.0",
    "npm": ">= 3.10.9"
  },
  "scripts": {
    "start": "webpack-dev-server --progress --https --display-error-details",
    "build": "webpack",
